call jcl "      EMAJ(GMAP) "
call jcl "CLASS NAME(IRRUMAP) "
call jcl "      EMAJ(UMAP) "
call jcl "CLASS NAME(IRRSMAP) "
call jcl "      EMAJ(SMAP) "

