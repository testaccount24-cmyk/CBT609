call jcl "      EMAJ(ACEE) "
call jcl "CLASS NAME(IKJEXEC) "
/* Add SYSPROCs */
DO x = 1 TO WORDS(sysproclibs)
   call jcl "      EDSN("WORD(sysproclibs,x)")"
END
call jcl "      MAXVIRT(256) "
